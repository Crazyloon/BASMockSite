@model IEnumerable<BASMockSite.Models.ProgramStructure>

@{
    ViewData["Title"] = "Index";
}

<h2>Index</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ProgramDuration)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Structure)
        </th>
        <th></th>
    </tr>
    
@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.ProgramDuration)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Structure)
        </td>
        <td>
            <a asp-action="Edit" asp-route-id="@item.ProgramStructureID">Edit</a> |
            <a asp-action="Details" asp-route-id="@item.ProgramStructureID">Details</a> |
            <a asp-action="Delete" asp-route-id="@item.ProgramStructureID">Delete</a>
        </td>
    </tr>
}
</table>
